<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="domain.Manager"
           table="Manager">
        <id name="username" column="username">
        </id>
        <property
                name="password"
                column="password"/>
        <property
                name="name"
                column="name"/>
    </class>
</hibernate-mapping>